{
  "name": "bun-typia-template",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "dev": "bun run --watch ./index.ts",
    "start": "bun run ./index.ts",
    "build": "bun run build.ts",
    "run": "node ./out/index.js",
    "prepare": "ts-patch install && typia patch && npx bunpare"
  },
  "devDependencies": {
    "@ryoppippi/unplugin-typia": "^1.1.0",
    "@types/bun": "latest",
    "ts-patch": "^3.3.0",
    "typescript": "~5.7.2"
  },
  "dependencies": {
    "typia": "^7.3.0"
  }
}
