    "run": "node ./out/index.js",
    "prepare": "ts-patch install && typia patch"
  },
  "devDependencies": {
    "@ryoppippi/unplugin-typia": "npm:@jsr/ryoppippi__unplugin-typia",
    "@types/bun": "latest",
    "ts-patch": "^3.2.0",
    "typescript": ">=4.8.0 <5.5.0"
  },
  "dependencies": {
    "typia": "^6.1.0"
  }
}
